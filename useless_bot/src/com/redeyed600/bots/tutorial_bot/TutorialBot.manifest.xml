<manifest>
    <main-class>com.redeyed600.bots.tutorial_bot.TutorialBot</main-class>
    <name>Tutorial Bot</name>
    <tag-line>Insert catchy tagline here (marketing oriented)</tag-line><!--Max of 50 chars-->
    <description>prints useless stuff</description><!--Max of 110 chars-->
    <version>1.0.2</version>
    <compatibility>
        <game>OSRS</game>
        <game>RS3</game>
    </compatibility>
    <categories>
        <category>DEVELOPER_TOOLS</category>
    </categories>
    <!--Required to publish on the bot store-->
    <internal-id>tutbot</internal-id>
    <!--The rest are optional-->
    <open-source>true</open-source>
    <hidden>false</hidden> <!--If you need to hide it from the bot store for maintenance-->
    <access>public</access>
    <tags>
        <tag>Tree</tag>
        <tag>Vine</tag>
    </tags>
    <resources>
        <resource>path/to/file.png</resource>
    </resources>
</manifest>